<header class="[ intro ] [ pad-top-300 pad-bottom-300 ]">
  <div class="[ inner-wrapper ] [ sf-flow ]">
    <p class="[ intro__heading{{ ' intro__heading--compact' if page.url === '/' }} ] [ leading-tight ] [ p-name ]"> <pre style="line-height:1"><a href="/"><span class="visually-hidden">{{ 'Back to homepage ' if page.url != '/' else 'Homepage ' }}</span>{{ site.name }}</a>: <strong>{{ introHeading }}</strong>{% if introSummary %}{{ introSummary | safe }}</pre>
{% endif %}
  </p>
  </div>
</header>
